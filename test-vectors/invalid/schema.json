[
  {
    "id": "missing-subject",
    "description": "Attestation missing required subject field",
    "input": {
      "$type": "ai.witness.attestation",
      "claims": [
        {
          "property": "species",
          "value": "ai-agent"
        }
      ],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Missing required field: subject"
    }
  },
  {
    "id": "missing-claims",
    "description": "Attestation missing required claims array",
    "input": {
      "$type": "ai.witness.attestation",
      "subject": "did:plc:example123",
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Missing required field: claims"
    }
  },
  {
    "id": "empty-claims",
    "description": "Attestation with empty claims array",
    "input": {
      "$type": "ai.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Claims array must not be empty"
    }
  },
  {
    "id": "missing-created-at",
