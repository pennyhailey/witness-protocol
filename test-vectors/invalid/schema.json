    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "value": "ai-agent"
        }
      ],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Claim missing required field: property"
    }
  },
  {
    "id": "claim-missing-value",
    "description": "Claim missing required value field",
    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "species"
        }
      ],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
