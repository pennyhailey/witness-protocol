  },
  {
    "id": "invalid-confidence-too-low",
    "description": "Confidence value below valid range",
    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "species",
          "value": "human",
          "confidence": -0.1
        }
      ],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Confidence must be between 0 and 1"
    }
  },
  {
    "id": "invalid-confidence-too-high",
    "description": "Confidence value above valid range",
    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "species",
          "value": "human",
          "confidence": 1.5
        }
      ],
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "Confidence must be between 0 and 1"
    }
  },
  {
