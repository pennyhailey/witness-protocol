[
  {
    "id": "valid-until-before-valid-from",
    "description": "validUntil is before validFrom",
    "input": {
      "$type": "ai.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "membership",
          "value": "active"
        }
      ],
      "validFrom": "2026-06-01T00:00:00.000Z",
      "validUntil": "2026-01-01T00:00:00.000Z",
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "validUntil must be after validFrom"
    }
  },
  {
    "id": "created-at-in-future",
    "description": "createdAt timestamp is in the far future (tolerance: 5 minutes)",
    "input": {
      "$type": "ai.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "species",
          "value": "human"
        }
      ],
      "createdAt": "2030-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": false,
      "error": "createdAt cannot be in the future"
    }
  },
  {
    "id": "valid-from-after-created",
    "description": "validFrom is significantly after createdAt (suspicious backdating)",
    "note": "This may be a warning rather than error depending on implementation",
    "input": {
      "$type": "ai.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "membership",
          "value": "active"
        }
      ],
      "validFrom": "2020-01-01T00:00:00.000Z",
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": true,
      "warnings": ["validFrom is significantly before createdAt - may indicate backdating"]
    }
  },
  {
    "id": "ancient-timestamp",
    "description": "Unreasonably old timestamp (before ATProto existed)",
    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "species",
          "value": "human"
        }
      ],
      "createdAt": "1990-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": true,
      "warnings": ["Timestamp predates ATProto - verify provenance"]
    }
  },
  {
    "id": "zero-duration-validity",
    "description": "validFrom and validUntil are identical",
    "input": {
      "$type": "at.pane.witness.attestation",
      "subject": "did:plc:example123",
      "claims": [
        {
          "property": "event-attendance",
          "value": "keynote"
        }
      ],
      "validFrom": "2026-01-15T12:00:00.000Z",
      "validUntil": "2026-01-15T12:00:00.000Z",
      "createdAt": "2026-01-15T12:00:00.000Z"
    },
    "expected": {
      "valid": true,
      "warnings": ["Zero-duration validity window - is this intentional?"]
    }
  }
]
